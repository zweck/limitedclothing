//-
	@file Demo page for the parallax_header module
	@author Phil Hauser phil.hsr@gmail.com
	@extends page_templates/_factory.jade
	@requires mixins/parallax_header/[MODFILE]

//- Page dependencies
include _parallax_header

//- Page template
extends ../../templates/_page_default/_page_default

//- Page variables to pass to template
block page_vars
	- var page = {}
	- page.id = "parallax_header"
	- page.title = "parallax_header"
	- page.mainScript = "parallax_header.js"


//- Content block
block content

	+parallax_header()
